---
title: Change Me
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{title}}</title>

    <!-- fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Montserrat+Subrayada&family=Montserrat:wght@400;700&family=Special+Elite&display=swap"
      rel="stylesheet"
    />

    <!-- my styles -->
    <link rel="stylesheet" href="~/node_modules/normalize.css/normalize.css" />
    <link rel="stylesheet" href="~/site/main.css" />

    <!-- Favicon stuff -->
    <link
      rel="apple-touch-icon"
      sizes="180x180"
      href="~/site/manifest/apple-touch-icon.png"
    />
    <link
      rel="icon"
      type="image/png"
      sizes="32x32"
      href="~/site/manifest/favicon-32x32.png"
    />
    <link
      rel="icon"
      type="image/png"
      sizes="16x16"
      href="~/site/manifest/favicon-16x16.png"
    />
    <link rel="manifest" href="~/site/manifest/site.webmanifest" />
  </head>
  <body>
    {{ content }}
  </body>
</html>
